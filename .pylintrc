[MASTER]
extension-pkg-whitelist=tkinter

[MESSAGES CONTROL]
disable=
    C0103,  # invalid-name
    C0301,  # line-too-long
    R0903,  # too-few-public-methods
    W0613,  # unused-argument
    C0114,  # missing-module-docstring
    C0115,  # missing-class-docstring
    C0116,  # missing-function-docstring
    W0718,  # broad-exception-caught

[BASIC]
good-names=
    i,j,k,ex,Run,_,
    root,app,e,f,df,tk

[FORMAT]
max-line-length=120